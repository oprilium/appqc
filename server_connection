import pyodbc
import pandas as pd

conn = pyodbc.connect('Driver={SQL Server};'
                      'Server=*********;'
                      'Database=********;'
                      'Trusted_Connection=yes;')

#### To write_sql conn must have user and pass, which in this needs to be a general purpose user #### 
#### Replace ***** for desired info
cursor = conn.cursor()
cursor.execute()

pd.read_sql('SELECT TOP 10* FROM TAB_*********',conn)

